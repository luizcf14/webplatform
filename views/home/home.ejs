
<html lang="pt-br">

<head>
    
    <title>Wep Application</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <!-- Jquery -->
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <!-- Leaflet CSS -->
    <link rel="stylesheet" href="https://unpkg.com/leaflet@1.3.1/dist/leaflet.css" integrity="sha512-Rksm5RenBEKSKFjgI3a41vrjkw4EVPlJ3+OiI65vTjIdo9brlAacEuKOiQ5OFh7cOI1bkDwLqdLw3Zg0cRJAAQ==" crossorigin="" />
    <!-- Leaflet JavaScript -->
    <script src="https://unpkg.com/leaflet@1.3.1/dist/leaflet.js" integrity="sha512-/Nsx9X4HebavoBvEBuyp3I7od5tA0UzAxs+j83KgC8PU0kgB4XiK4Lfe4y4cgBtaRJQEIFCW+oC506aPT2L1zw==" crossorigin=""></script>
    <!-- Google Charts -->
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"> </script>
    
    <link href="css/home/home.css" rel="stylesheet" type="text/css">

</head>

<body>
    <div id="navTop">
	<nav class="navbar navbar-expand-lg navbar-light bg-light navbar-fixed-top">
        	<div class="container-fluid">
            		<div class="navbar-header">
                		<a class="navbar-brand" href="#">Mangrove Platform Beta</a>
            		</div>
        	</div>
    	</nav>
    </div>
    <div id="container">        
        <div id="mapid"></div>
        <!-- Modal -->
        <div class="modal fade" id="TVGraph" tabindex="-1" role="dialog" aria-labelledby="TVL" aria-hidden="true">
                <div class="modal-dialog modal-lg" role="document">
                    <div class="modal-content" id="graph">
                        <div class="modal-header">
                            <h5 class="modal-title" id="TVL">Variação Temporal</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div id="chart_div"></div>      
                        </div>
                        
                        <div id="saveGraphPNG" class="modal-footer d-none">
                            <!--<button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>-->
                            <button type="button" class="btn btn-primary" onclick="savePNG()">Salvar PNG</button>
                        </div>
                        
                    </div>
                </div>
            </div>
    </div>  
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl"
        crossorigin="anonymous"></script>
    <script>
            google.charts.load('current', {packages: ['corechart', 'line']});
            function getGoogleVizualization(type)
            {
                switch(type){
                    case "DataTable":
                        return new google.visualization.DataTable();                        
                    case "LineChart":
                        return new google.visualization.LineChart(document.getElementById('chart_div'));
                }            
            }
        </script>    
        <script  src="scripts/home/home.js"></script>
</body>

</html>
